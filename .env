 # ========================================
# SLA MONITORING CONFIGURATION
# ========================================
 
# SLA monitoring interval (cron format)
# Default: */5 * * * * (every 5 minutes)
# Examples:
# */1 * * * * = every 1 minute
# */10 * * * * = every 10 minutes
# 0 */1 * * * = every hour
# 0 0 */6 * * = every 6 hours
SLA_MONITORING_INTERVAL=*/1 * * * *
 
# SLA targets in hours for each priority level
SLA_P1_HOURS=4
SLA_P2_HOURS=12
SLA_P3_HOURS=24
 
# SLA notification thresholds (percentage of time elapsed)
SLA_WARNING_THRESHOLD=20
SLA_CRITICAL_THRESHOLD=60
 
# Enable/disable SLA monitoring
SLA_MONITORING_ENABLED=true
 
# Process all SLA records regardless of status (set to true to monitor all tickets)
SLA_IGNORE_STATUS_FILTER=true
 
 
 
